{
  "version": 1,
  "projects": {
    "pos": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/pos",
      "sourceRoot": "apps/pos/src",
      "prefix": "pa-pos",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/pos",
            "index": "apps/pos/src/index.html",
            "main": "apps/pos/src/main.ts",
            "polyfills": "apps/pos/src/polyfills.ts",
            "tsConfig": "apps/pos/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/pos/src/favicon.ico", "apps/pos/src/assets"],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "apps/pos/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/pos/src/environments/environment.ts",
                  "with": "apps/pos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pos:build",
            "proxyConfig": "apps/pos/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "pos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pos:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/pos/tsconfig.app.json",
              "apps/pos/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/pos/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/pos/jest.config.js",
            "tsConfig": "apps/pos/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/pos/src/test-setup.ts"
          }
        }
      }
    },
    "pos-e2e": {
      "root": "apps/pos-e2e",
      "sourceRoot": "apps/pos-e2e/src",
      "projectType": "application",
      "architect": {
